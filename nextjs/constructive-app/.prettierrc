{
	"jsxSingleQuote": true,
	"printWidth": 120,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"semi": true,
	"useTabs": true,
	"plugins": [
		"@ianvs/prettier-plugin-sort-imports",
		"prettier-plugin-tailwindcss",
		"prettier-plugin-classnames",
		"prettier-plugin-merge"
	],
	"importOrder": [
		"^(react/(.*)$)|^(react$)",
		"^(next/(.*)$)|^(next$)",
		"<THIRD_PARTY_MODULES>",
		"",
		"^@/types/(.*)$",
		"^@/config(.*)$",
		"^@/(lib|utils|hooks|store)/(.*)$",
		"^@/components/ui/(.*)$",
		"^@/components/(.*)$",
		"^@/(.*)$",
		"",
		"^[./]"
	],
	"importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
